# JamaiBase API Configuration
# Copy this file to .env and fill in your actual values

# API URL for JamaiBase (e.g., https://cloud.jamaibase.com)
JAMAI_API_URL=https://cloud.jamaibase.com

# API Key for authentication
JAMAI_API_KEY=your_api_key_here

# Test Organization ID (for organization-specific tests)
TEST_ORGANIZATION_ID=your_test_organization_id

# Test Project ID (for project-specific tests)
TEST_PROJECT_ID=your_test_project_id

# Test User ID (for user-related tests)
TEST_USER_ID=your_test_user_id